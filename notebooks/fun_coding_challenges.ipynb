   "source": [
    "def create_diamond_pattern(size):\n",
    "    \"\"\"Create a diamond pattern using ASCII characters.\"\"\"\n",
    "    pattern = []\n",
    "    \n",
    "    # Top half\n",
    "    for i in range(size):\n",
    "        spaces = \" \" * (size - i - 1)\n",
    "        stars = \"*\" * (2 * i + 1)\n",
    "        pattern.append(spaces + stars)\n",
    "    \n",
    "    # Bottom half\n",
    "    for i in range(size - 2, -1, -1):\n",
    "        spaces = \" \" * (size - i - 1)\n",
    "        stars = \"*\" * (2 * i + 1)\n",
    "        pattern.append(spaces + stars)\n",
    "    \n",
    "    return \"\\n\".join(pattern)\n",
    "\n",
    "def create_heart_pattern():\n",
    "    \"\"\"Create a heart pattern.\"\"\"\n",
    "    heart = [\n",
    "        \"  ❤️   ❤️  \",\n",
    "        \"❤️    ❤️\",\n",
    "        \"❤️      ❤️\",\n",
    "        \"  ❤️  ❤️  \",\n",
    "        \"    ❤️    \"\n",
    "    ]\n",
    "    return \"\\n\".join(heart)\n",
    "\n",
    "def create_wave_pattern(width, height):\n",
    "    \"\"\"Create a wave pattern.\"\"\"\n",
    "    pattern = []\n",
    "    for y in range(height):\n",
    "        line = \"\"\n",
    "        for x in range(width):\n",
    "            # Create wave using sine function\n",
    "            import math\n",
    "            wave = math.sin((x + y) * 0.5) * 2\n",
    "            if abs(wave) < 0.5:\n",
    "                line += \"~\"\n",
    "            else:\n",
    "                line += \" \"\n",
    "        pattern.append(line)\n",
    "    return \"\\n\".join(pattern)\n",
    "\n",
    "# Test the patterns\n",
    "print(\"Diamond Pattern:\")\n",
    "print(create_diamond_pattern(5))\n",
    "print(\"\\n\" + \"=\"*20 + \"\\n\")\n",
    "\n",
    "print(\"Heart Pattern:\")\n",
    "print(create_heart_pattern())\n",
    "print(\"\\n\" + \"=\"*20 + \"\\n\")\n",
    "\n",
    "print(\"Wave Pattern:\")\n",
    "print(create_wave_pattern(20, 8))"
   ]
