    "def play_text_adventure():\n",
    "    \"\"\"A simple text-based adventure game.\"\"\"\n",
    "    \n",
    "    # Game state\n",
    "    inventory = []\n",
    "    current_room = \"entrance\"\n",
    "    \n",
    "    # Game world\n",
    "    rooms = {\n",
    "        \"entrance\": {\n",
    "            \"description\": \"You stand at the entrance of an ancient castle. A door leads north.\",\n",
    "            \"exits\": {\"north\": \"hall\"},\n",
    "            \"items\": [\"key\"]\n",
    "        },\n",
    "        \"hall\": {\n",
    "            \"description\": \"A grand hall with a staircase leading up. Doors lead south and east.\",\n",
    "            \"exits\": {\"south\": \"entrance\", \"east\": \"library\", \"up\": \"tower\"},\n",
    "            \"items\": [\"sword\"]\n",
    "        },\n",
    "        \"library\": {\n",
    "            \"description\": \"A dusty library filled with ancient books. A door leads west.\",\n",
    "            \"exits\": {\"west\": \"hall\"},\n",
    "            \"items\": [\"book\"]\n",
    "        },\n",
    "        \"tower\": {\n",
    "            \"description\": \"The tower room. A window shows a dragon outside!\",\n",
    "            \"exits\": {\"down\": \"hall\"},\n",
    "            \"items\": []\n",
    "        }\n",
    "    }\n",
    "    \n",
    "    def show_status():\n",
    "        \"\"\"Show current game status.\"\"\"\n",
    "        room = rooms[current_room]\n",
    "        print(f\"\\n🏰 {room['description']}\")\n",
    "        \n",
    "        if room[\"items\"]:\n",
    "            print(f\"📦 You see: {', '.join(room['items'])}\")\n",
    "        \n",
    "        if inventory:\n",
    "            print(f\"🎒 Inventory: {', '.join(inventory)}\")\n",
    "        \n",
    "        exits = list(room[\"exits\"].keys())\n",
    "        print(f\"🚪 Exits: {', '.join(exits)}\")\n",
    "    \n",
    "    print(\"🗡️ Welcome to Castle Adventure!\")\n",
    "    print(\"Commands: go <direction>, take <item>, inventory, help, quit\")\n",
    "    \n",
    "    while True:\n",
    "        show_status()\n",
    "        \n",
    "        command = input(\"\\n> \").lower().split()\n",
    "        \n",
    "        if not command:\n",
    "            continue\n",
    "        \n",
    "        action = command[0]\n",
    "        \n",
    "        if action == \"quit\":\n",
    "            print(\"👋 Thanks for playing!\")\n",
    "            break\n",
    "        \n",
    "        elif action == \"help\":\n",
    "            print(\"Commands:\")\n",
    "            print(\"  go <direction> - Move to another room\")\n",
    "            print(\"  take <item> - Pick up an item\")\n",
    "            print(\"  inventory - Show your items\")\n",
    "            print(\"  look - Look around\")\n",
    "            print(\"  quit - Exit game\")\n",
    "        \n",
    "        elif action == \"go\" and len(command) > 1:\n",
    "            direction = command[1]\n",
    "            room = rooms[current_room]\n",
    "            \n",
    "            if direction in room[\"exits\"]:\n",
    "                current_room = room[\"exits\"][direction]\n",
    "                print(f\"You go {direction}.\")\n",
    "            else:\n",
    "                print(\"You can't go that way!\")\n",
    "        \n",
    "        elif action == \"take\" and len(command) > 1:\n",
    "            item = command[1]\n",
    "            room = rooms[current_room]\n",
    "            \n",
    "            if item in room[\"items\"]:\n",
    "                room[\"items\"].remove(item)\n",
    "                inventory.append(item)\n",
    "                print(f\"You take the {item}.\")\n",
    "            else:\n",
    "                print(\"That item isn't here!\")\n",
    "        \n",
    "        elif action == \"inventory\":\n",
    "            if inventory:\n",
    "                print(f\"You have: {', '.join(inventory)}\")\n",
    "            else:\n",
    "                print(\"Your inventory is empty.\")\n",
    "        \n",
    "        elif action == \"look\":\n",
    "            show_status()\n",
    "        \n",
    "        else:\n",
    "            print(\"I don't understand that command. Type 'help' for commands.\")\n",
    "\n",
    "# Demo the game structure\n",
    "print(\"Castle Adventure Game Structure:\")\n",
    "print(\"🏰 Entrance -> Hall -> Library\")\n",
    "print(\"                -> Tower\")\n",
    "print(\"\\nItems to collect: key, sword, book\")\n",
    "print(\"\\nTry the full game by uncommenting:\")\n",
    "# play_text_adventure()"
   ]
