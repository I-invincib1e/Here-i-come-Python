    "def create_sierpinski_triangle(size):\n",
    "    \"\"\"Create a Sierpinski triangle pattern.\"\"\"\n",
    "    if size == 0:\n",
    "        return [\"*\"]\n",
    "    \n",
    "    # Recursively create smaller triangles\n",
    "    smaller = create_sierpinski_triangle(size - 1)\n",
    "    \n",
    "    # Create top triangle\n",
    "    top = [line.center(len(smaller[0]) * 2) for line in smaller]\n",
    "    \n",
    "    # Create bottom triangles\n",
    "    bottom = [line + \" \" + line for line in smaller]\n",
    "    \n",
