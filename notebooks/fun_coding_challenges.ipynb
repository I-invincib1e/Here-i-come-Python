    "def create_pascal_triangle(rows):\n",
    "    \"\"\"Generate Pascal's triangle.\"\"\"\n",
    "    triangle = []\n",
    "    \n",
    "    for i in range(rows):\n",
    "        row = [1] * (i + 1)\n",
    "        for j in range(1, i):\n",
    "            row[j] = triangle[i-1][j-1] + triangle[i-1][j]\n",
    "        triangle.append(row)\n",
    "    \n",
    "    return triangle\n",
    "\n",
    "def print_pascal_triangle(triangle):\n",
    "    \"\"\"Print Pascal's triangle with proper formatting.\"\"\"\n",
    "    max_width = len(' '.join(map(str, triangle[-1])))\n",
    "    \n",
    "    for row in triangle:\n",
    "        row_str = ' '.join(map(str, row))\n",
    "        print(row_str.center(max_width))\n",
    "\n",
    "def create_fibonacci_spiral(size):\n",
    "    \"\"\"Create a Fibonacci spiral pattern.\"\"\"\n",
    "    # Generate Fibonacci numbers\n",
    "    fib = [1, 1]\n",
    "    for i in range(size - 2):\n",
    "        fib.append(fib[-1] + fib[-2])\n",
    "    \n",
    "    # Create spiral pattern\n",
    "    pattern = []\n",
    "    for i, num in enumerate(fib):\n",
    "        direction = i % 4\n",
    "        if direction == 0:\n",
    "            pattern.append(f\"→ {num}\")\n",
    "        elif direction == 1:\n",
    "            pattern.append(f\"↓ {num}\")\n",
    "        elif direction == 2:\n",
    "            pattern.append(f\"← {num}\")\n",
    "        else:\n",
    "            pattern.append(f\"↑ {num}\")\n",
    "    \n",
    "    return pattern\n",
    "\n",
    "def create_multiplication_table(size):\n",
    "    \"\"\"Create a colorful multiplication table.\"\"\"\n",
    "    table = []\n",
    "    \n",
    "    # Header\n",
    "    header = [\"×\"] + [str(i) for i in range(1, size + 1)]\n",
    "    table.append(header)\n",
    "    \n",
    "    # Rows\n",
    "    for i in range(1, size + 1):\n",
    "        row = [str(i)]\n",
    "        for j in range(1, size + 1):\n",
    "            result = i * j\n",
    "            if result % 2 == 0:\n",
    "                row.append(f\"{result}*\")  # Mark even numbers\n",
    "            else:\n",
    "                row.append(str(result))\n",
    "        table.append(row)\n",
    "    \n",
    "    return table\n",
    "\n",
    "def print_table(table):\n",
    "    \"\"\"Print a table with proper formatting.\"\"\"\n",
    "    col_widths = [max(len(str(cell)) for cell in col) for col in zip(*table)]\n",
    "    \n",
    "    for row in table:\n",
    "        formatted_row = []\n",
    "        for i, cell in enumerate(row):\n",
    "            formatted_row.append(str(cell).rjust(col_widths[i]))\n",
    "        print(' │ '.join(formatted_row))\n",
    "    \n",
    "    # Print separator\n",
    "    separator = '─'.join('─' * width for width in col_widths)\n",
    "    print('─' + separator + '─')\n",
    "\n",
    "# Test the patterns\n",
    "print(\"Pascal's Triangle:\")\n",
    "pascal = create_pascal_triangle(8)\n",
    "print_pascal_triangle(pascal)\n",
    "\n",
    "print(\"\\n\" + \"=\"*30 + \"\\n\")\n",
    "\n",
    "print(\"Fibonacci Spiral:\")\n",
    "spiral = create_fibonacci_spiral(10)\n",
    "for step in spiral:\n",
    "    print(step)\n",
    "\n",
    "print(\"\\n\" + \"=\"*30 + \"\\n\")\n",
    "\n",
    "print(\"Multiplication Table:\")\n",
    "mult_table = create_multiplication_table(5)\n",
    "print_table(mult_table)"
   ]
