    "def get_hangman_art(attempts_left):\n",
    "    \"\"\"Get ASCII art for hangman based on attempts left.\"\"\"\n",
    "    stages = [\n",
    "        \"\"\"\n",
    "           +---+\n",
    "           |   |\n",
    "           O   |\n",
    "          /|\\  |\n",
    "          / \\  |\n",
    "               |\n",
    "        ========\"\"\",\n",
    "        \"\"\"\n",
    "           +---+\n",
    "           |   |\n",
    "           O   |\n",
    "          /|\\  |\n",
    "          /    |\n",
    "               |\n",
    "        ========\"\"\",\n",
    "        \"\"\"\n",
    "           +---+\n",
    "           |   |\n",
    "           O   |\n",
    "          /|\\  |\n",
    "               |\n",
    "               |\n",
    "        ========\"\"\",\n",
    "        \"\"\"\n",
    "           +---+\n",
    "           |   |\n",
    "           O   |\n",
    "          /|   |\n",
    "               |\n",
    "               |\n",
    "        ========\"\"\",\n",
    "        \"\"\"\n",
    "           +---+\n",
    "           |   |\n",
    "           O   |\n",
    "           |   |\n",
    "               |\n",
    "               |\n",
    "        ========\"\"\",\n",
    "        \"\"\"\n",
    "           +---+\n",
    "           |   |\n",
    "           O   |\n",
    "               |\n",
    "               |\n",
    "               |\n",
    "        ========\"\"\",\n",
    "        \"\"\"\n",
    "           +---+\n",
    "           |   |\n",
    "               |\n",
    "               |\n",
    "               |\n",
    "               |\n",
    "        ========\"\"\"\n",
    "    ]\n",
    "    return stages[6 - attempts_left]\n",
    "\n",
    "def play_hangman():\n",
    "    \"\"\"Play a game of hangman.\"\"\"\n",
    "    # Word list for the game\n",
    "    words = [\"PYTHON\", \"COMPUTER\", \"PROGRAMMING\", \"ALGORITHM\", \"FUNCTION\"]\n",
    "    \n",
    "    import random\n",
    "    word = random.choice(words)\n",
    "    word_letters = set(word)\n",
    "    guessed_letters = set()\n",
    "    attempts_left = 6\n",
    "    \n",
    "    print(\"🎯 Welcome to Hangman!\")\n",
    "    print(f\"Word has {len(word)} letters\")\n",
    "    \n",
    "    while attempts_left > 0 and word_letters:\n",
    "        print(\"\\n\" + get_hangman_art(attempts_left))\n",
    "        \n",
    "        # Show current word state\n",
    "        current_word = \"\"\n",
    "        for letter in word:\n",
    "            if letter in guessed_letters:\n",
    "                current_word += letter\n",
    "            else:\n",
    "                current_word += \"_\"\n",
    "        \n",
    "        print(f\"\\nWord: {current_word}\")\n",
    "        print(f\"Guessed: {' '.join(sorted(guessed_letters))}\")\n",
    "        print(f\"Attempts left: {attempts_left}\")\n",
    "        \n",
    "        guess = input(\"\\nGuess a letter: \").upper()\n",
    "        \n",
    "        if len(guess) != 1 or not guess.isalpha():\n",
    "            print(\"Please enter a single letter!\")\n",
    "            continue\n",
    "        \n",
    "        if guess in guessed_letters:\n",
    "            print(\"You already guessed that letter!\")\n",
    "            continue\n",
    "        \n",
    "        guessed_letters.add(guess)\n",
    "        \n",
    "        if guess in word_letters:\n",
    "            word_letters.remove(guess)\n",
    "            print(f\"✅ Good guess! '{guess}' is in the word.\")\n",
    "        else:\n",
    "            attempts_left -= 1\n",
    "            print(f\"❌ Wrong guess! '{guess}' is not in the word.\")\n",
    "    \n",
    "    print(\"\\n\" + get_hangman_art(attempts_left))\n",
    "    \n",
    "    if not word_letters:\n",
    "        print(f\"🎉 Congratulations! You guessed the word: {word}\")\n",
    "    else:\n",
    "        print(f\"😞 Game over! The word was: {word}\")\n",
    "    \n",
    "    return not word_letters\n",
    "\n",
    "# Demo the hangman art\n",
    "print(\"Hangman Stages:\")\n",
    "for i in range(7):\n",
    "    print(f\"\\nAttempts left: {6-i}\")\n",
    "    print(get_hangman_art(6-i))\n",
    "\n",
    "print(\"\\nTry the full game by uncommenting:\")\n",
    "# play_hangman()"
   ]
